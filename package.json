{
  "name": "my-app",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && sudo PORT=80 npm run start",
    "server": "nodemon server.js --ignore client",
    "dev": "concurrently  \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "proxy": {
    "/api/*": {
      "target": "http://localhost:5000"
    }
  }
}
